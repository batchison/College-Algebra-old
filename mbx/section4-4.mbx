\tlabel[cc](1,1.5){\scriptsize <m>(1,1)</m>}
\tlabel[cc](4,2.5){\scriptsize <m>(4,2)</m>}
\tlabel[cc](5,-0.5){\scriptsize <m>x</m>}
\tlabel[cc](0.5,4){\scriptsize <m>y</m>}
