\ymarks{-4,-3,-2,-1,1,2,3,4,5,6}
\tlpointsep{5pt}
\tcaption{graph of <m>f</m>}
\axislabels {x}{{\scriptsize <m>-4 \</m>} -4, {\scriptsize <m>-3 \</m>} -3, {\scriptsize <m>-2 \</m>} -2, {\scriptsize <m>-1 \</m>} -1, {\scriptsize <m>1</m>} 1, {\scriptsize <m>2</m>} 2, {\scriptsize<m>3</m>} 3, {\scriptsize <m>4</m>} 4}
\axislabels {y}{{\scriptsize <m>-4</m>} -4, {\scriptsize <m>-3</m>} -3, {\scriptsize <m>-2</m>} -2, {\scriptsize <m>-1</m>} -1, {\scriptsize <m>1</m>} 1, {\scriptsize <m>2</m>} 2, {\scriptsize <m>3</m>} 3, {\scriptsize <m>4</m>} 4, {\scriptsize <m>5</m>} 5, {\scriptsize <m>6</m>} 6}
\end{mfpic}

\amp 
\hspace{.5in}
\amp 
\begin{mfpic}[13]{-5}{5}{-4}{7}
\point[3pt]{(-1.5,6.5), (-0.5,2), (0.5,-2.5), (1.5,2), (2.5,6.5)}
